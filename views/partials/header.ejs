    <script>
        (function() {
            'use strict';
            
            function initDropdowns() {
                console.log('[Dropdown] Initializing...');
                const navItems = document.querySelectorAll('.theader-nav-item');
                console.log('[Dropdown] Found', navItems.length, 'nav items');
                
                if (navItems.length === 0) {
                    console.error('[Dropdown] No nav items found!');
                    return;
                }
                
                navItems.forEach(function(item, index) {
                    const navLink = item.querySelector('.theader-nav-link');
                    const dropdown = item.querySelector('.theader-top-menu-content');
                    
                    if (!navLink || !dropdown) {
                        console.warn('[Dropdown] Item', index, 'missing link or dropdown');
                        return;
                    }
                    
                    const linkText = navLink.textContent.trim();
                    console.log('[Dropdown] Setting up:', linkText);
                    
                    // Ensure dropdown is initially hidden with !important override
                    dropdown.style.setProperty('display', 'none', 'important');
                    
                    // Track state
                    let isOpen = false;
                    
                    // Direct click handler - don't clone, just add listener
                    navLink.addEventListener('click', function(e) {
                        console.log('[Dropdown] CLICKED:', linkText);
                        e.preventDefault();
                        e.stopPropagation();
                        
                        // Close all other dropdowns
                        navItems.forEach(function(otherItem) {
                            if (otherItem !== item) {
                                otherItem.classList.remove('theader-active');
                                const otherDropdown = otherItem.querySelector('.theader-top-menu-content');
                                if (otherDropdown) {
                                    otherDropdown.style.setProperty('display', 'none', 'important');
                                }
                            }
                        });
                        
                        // Toggle current dropdown
                        if (isOpen) {
                            console.log('[Dropdown] Closing:', linkText);
                            item.classList.remove('theader-active');
                            dropdown.style.setProperty('display', 'none', 'important');
                            isOpen = false;
                        } else {
                            console.log('[Dropdown] Opening:', linkText);
                            item.classList.add('theader-active');
                            
                            // Remove all display restrictions
                            dropdown.style.removeProperty('display');
                            dropdown.style.removeProperty('visibility');
                            dropdown.style.removeProperty('opacity');
                            
                            // Set display properties
                            dropdown.style.display = 'block';
                            dropdown.style.visibility = 'visible';
                            dropdown.style.opacity = '1';
                            dropdown.style.zIndex = '1001';
                            dropdown.style.position = 'absolute';
                            
                            isOpen = true;
                            
                            // Force reflow
                            void dropdown.offsetHeight;
                            
                            // Verify and log all computed styles
                            const finalStyle = window.getComputedStyle(dropdown);
                            const rect = dropdown.getBoundingClientRect();
                            
                            console.log('[Dropdown] Opened - Display:', finalStyle.display, 
                                      'Visibility:', finalStyle.visibility, 
                                      'Z-index:', finalStyle.zIndex,
                                      'Position:', finalStyle.position,
                                      'Top:', finalStyle.top,
                                      'Left:', finalStyle.left);
                            
                            console.log('[Dropdown] Bounding rect:', {
                                top: rect.top,
                                left: rect.left,
                                width: rect.width,
                                height: rect.height,
                                visible: rect.width > 0 && rect.height > 0
                            });
                            
                            console.log('[Dropdown] Element:', dropdown);
                            console.log('[Dropdown] Has content:', dropdown.children.length, 'children');
                            
                            if (finalStyle.display === 'none') {
                                console.error('[Dropdown] ERROR: Dropdown still hidden!');
                            }
                            
                            if (rect.width === 0 || rect.height === 0) {
                                console.error('[Dropdown] ERROR: Dropdown has no size!');
                            }
                        }
                    }, true); // Use capture phase
                    
                    // Hover handler
                    item.addEventListener('mouseenter', function() {
                        if (!isOpen) {
                            dropdown.style.removeProperty('display');
                            dropdown.style.display = 'block';
                            item.classList.add('theader-active');
                        }
                    });
                });
                
                // Close on outside click
                document.addEventListener('click', function(e) {
                    const clickedInside = e.target.closest('.theader-nav-item') || 
                                         e.target.closest('.theader-top-menu-content');
                    
                    if (!clickedInside) {
                        navItems.forEach(function(item) {
                            item.classList.remove('theader-active');
                            const dropdown = item.querySelector('.theader-top-menu-content');
                            if (dropdown) {
                                dropdown.style.setProperty('display', 'none', 'important');
                            }
                        });
                    }
                }, true);
                
                // Prevent closing when clicking inside dropdown
                document.querySelectorAll('.theader-top-menu-content').forEach(function(dropdown) {
                    dropdown.addEventListener('click', function(e) {
                        e.stopPropagation();
                    });
                });
                
                console.log('[Dropdown] Initialization complete');
            }
            
            // Initialize
            if (document.readyState === 'loading') {
                document.addEventListener('DOMContentLoaded', initDropdowns);
            } else {
                initDropdowns();
            }
            
            // Fallback initialization
            setTimeout(initDropdowns, 300);
        })();
    </script>
