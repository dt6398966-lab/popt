<div class="_99HomeLoanContentBox">
    <div class="_99HomeloanBxWrp">
        <div class="_99HomeloanBxWrp _loanEligiblty" id="checkEligibility">
            <div class="_99HomeLoanBordrBox _99HomeLoanForm wd100" style="margin-top: 0px;">
                <form action="" method="get" class="loanEligibleWrap">
                    <div class="loanEligibltyForm">
                        <div class="toggleContainer">
                            <span class="toggleBtnTxt">Number of Borrowers </span>
                            <div class="toggleBtn off" id="container">
                                <div class="switch" id="switch"></div>
                                <div class="label left">One</div>
