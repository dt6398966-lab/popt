                                    <li class="phoneCodeListItem" data-value="self-employed">Self Employed</li>
                                    <li class="phoneCodeListItem" data-value="business">Business</li>
                                </ul>
                                <span class="slidingInputLabel">Occupation</span>
                            </div>
                            <div class="eoiLyr_inpWrap wid50" data-attr="inputWrapper">
                                <span class="phnCodeValue netIncomeRupee">₹</span>
                                <span class="phnCodeValue hlYears" id="incomeUnit">Monthly</span>
                                <ul data-page="homeloan" class="phnCodeListing hide1" id="incomeUnitDropdown">
                                    <li class="phoneCodeListItem" data-value="monthly">Monthly</li>
                                    <li class="phoneCodeListItem" data-value="annually">Annually</li>
                                </ul>
                                <input class="__enableSubmitButton __phnNoVal pl10" data-inline="true" data-attr="inputText" data-role="none" type="text" data-mini="true" maxlength="10" value="1,00,000" id="netIncome">
                                <div class="inputTxtDisplay" id="netIncomeDisplay">One Lakhs </div>
                                <span class="slidingInputLabel netIncomeLabel">Net income</span>
                            </div>
                            <div class="eoiLyr_inpWrap wid50" data-attr="inputWrapper">
                                <span class="phnCodeValue existingEmiRupee">₹</span>
                                <input class="__enableSubmitButton __phnNoVal pl10" data-inline="true" data-attr="inputText" data-role="none" type="text" data-mini="true" maxlength="8" value="10,000" id="existingEmi">
